{
    "presets": [
        [
            "env",
            {
                "targets": [
                    {
                        "name": "node",
                        "version": 6
                    }
                ]
            }
        ]
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "polyfill": false,
                "regenerator": true
            }
        ],
        "transform-object-rest-spread",
        "transform-class-properties"
    ]
}